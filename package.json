  "homepage": "https://github.com/alajfit/vue-run#readme",
  "devDependencies": {
    "babel-eslint": "^8.2.5",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "parcel": "^1.9.4",
    "release-it": "^7.4.7",
    "vuepress": "^0.10.2"
